<?xml version="1.0" encoding="utf-8"?>
<resources>
<!--  自定义标题栏  -->
    <declare-styleable name="TitleBar">
        <attr name="bar_title_tv_center" format="string" />
        <attr name="bar_titie_tv_right" format="string" />
        <attr name="bar_titie_tv_right_icon" format="reference" />
        <attr name="bar_titie_tv_left" format="string" />
        <attr name="bar_titie_tv_left_icon" format="reference" />
        <attr name="bar_title_tv_center_color" format="color"/>
        <attr name="bar_title_tv_right_color" format="color"/>
        <attr name="bar_title_tv_left_color" format="color"/>
    </declare-styleable>


    <declare-styleable name="SettingTitleView">
        <attr name="settingName" format="string"/>
        <attr name="settingExtra" format="string"/>
        <attr name="settingExtraShow" format="boolean"/>
        <attr name="settingExtraDraw" format="reference"/>
        <attr name="settingBack" format="reference"/>
        <attr name="settingBackShow" format="boolean"/>
        <attr name="settinglineShow" format="boolean"/>
        <attr name="settingTitleRight" format="reference"/>
    </declare-styleable>


    <declare-styleable name="MateriaTextView">

        <attr name="isOut" format="boolean"/>
        <attr name="isBold" format="boolean"/>

    </declare-styleable>
</resources>